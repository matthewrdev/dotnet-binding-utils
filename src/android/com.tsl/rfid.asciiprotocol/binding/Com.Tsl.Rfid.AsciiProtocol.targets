<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <_ComTslRfidAsciiProtocol>true</_ComTslRfidAsciiProtocol>
  </PropertyGroup>  
  <PropertyGroup>
    <AndroidManifestMerger Condition=" '$(AndroidManifestMerger)' == '' ">manifestmerger.jar</AndroidManifestMerger>
  </PropertyGroup>
  <PropertyGroup>
    <AndroidFragmentType>AndroidX.Fragment.App.Fragment</AndroidFragmentType>
  </PropertyGroup>
  <ItemGroup>
    <AndroidLibrary 
      Pack="False"
      Bind="False"
      Visible="False"
      Include="$(ComTslRfidAsciiprotocolLibPath)" >
      <AndroidXSkipAndroidXMigration>true</AndroidXSkipAndroidXMigration>
    </AndroidLibrary>
  </ItemGroup>
</Project>